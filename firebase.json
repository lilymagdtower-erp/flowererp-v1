{
  "functions": {
    "source": ".",
    "runtime": "nodejs20",
    "predeploy": [
      "npm --prefix \"$RESOURCE_DIR\" install",
      "npm --prefix \"$RESOURCE_DIR\" run build"
    ],
    "ignore": [
      "node_modules",
      ".next",
      "src",
      "lib-old",
      "out",
      "*.log",
      "build"
    ]
  },
  "hosting": {
    "rewrites": [
      {
        "source": "**",
        "function": "serverV2"
      }
    ]
  }
}